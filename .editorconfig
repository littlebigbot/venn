# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# Set default charset
[*.{js,css}]
charset = utf-8

[*.js]
indent_style = space
indent_size = 2

[{package.json,.babelrc}]
indent_style = space
indent_size = 2

[*.css]
indent_style = space
indent_size = 2
